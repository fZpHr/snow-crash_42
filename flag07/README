int main()
{
    void* v0;  // [bp-0x10]
    unsigned int v1;  // [bp-0xc]
    unsigned int v2;  // [bp-0x8]

    v1 = getegid();
    v2 = geteuid();
    setresgid(v1, v1, v1);
    setresuid(v2, v2, v2);
    v0 = 0;
    getenv("LOGNAME");
    asprintf(&v0, "/bin/echo %s ");
    return system(v0);
}

